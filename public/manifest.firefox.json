{
  "manifest_version": 2,
  "name": "TabScape",
  "short_name": "TabScape",
  "version": "1.2.0",
  "description": "Replaces your start- or new tab page with a minimalistic clock and beautiful backgrounds.",
  "author": "Freddie Lust-Hed",
  "homepage_url": "https://github.com/Musenkishi/nutab",
  "icons": {
    "64": "tabscape.svg",
    "192": "tabscape.svg",
    "512": "tabscape.svg"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "chrome_settings_overrides": {
    "homepage": "index.html"
  }
}
